#include<iostream>
using namespace std;

int main(int argc, char** argv)
{
	int test_case, T, n, m, money;
	float p, result;
	cin >> T;
	for (test_case = 1; test_case <= T; ++test_case)
	{
		cin >> n;
		result = 0;
		for (int i = 0; i < n; i++) {
			cin >> p >> money;
			result += p * money;
		}
		printf("#%d %6f", test_case, result);
	}
	return 0;
}
